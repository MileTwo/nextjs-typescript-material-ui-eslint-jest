        "react-hook-form": "^7.12.2",
        "reflect-metadata": "^0.1.13",
        "type-graphql": "^1.1.1"
    },
    "scripts": {
        "build": "next build",
        "dev": "prisma generate && concurrently \"next\" \"npm run watch-queries\"",
        "format": "prisma format &&  prettier --write . && eslint --fix .",
        "lint": "eslint .",
        "start": "next start",
        "test": "jest",
        "type-check": "tsc",
        "quality": "npm run type-check && npm run lint && npm run test",
        "generate": "prisma generate",
        "migrate": "prisma migrate dev",
        "deploy": "prisma migrate deploy",
        "reset": "prisma migrate reset",
        "seed": "prisma db seed --preview-feature",
        "studio": "prisma studio",
        "prisma": "prisma format && npm run migrate && npm run seed",
        "generate-types": "graphql-codegen --config codegen.yml",
        "watch-queries": "graphql-codegen --config codegen.yml --watch",
        "postinstall": "prisma generate"
    },
    "devDependencies": {
        "@axe-core/react": "^4.2.2",
        "@babel/core": "^7.15.0",
        "@babel/node": "^7.14.9",
