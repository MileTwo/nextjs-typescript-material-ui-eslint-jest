{
    "name": "nextjs-typescript-material-ui-eslint-jest",
    "version": "1.0.1",
    "private": true,
    "dependencies": {
        "@apollo/client": "^3.3.19",
        "@material-ui/core": "^4.11.4",
        "@prisma/client": "^2.25.0",
        "@types/graphql-fields": "^1.3.3",
        "apollo-server-micro": "^2.24.1",
        "graphql": "^15.5.0",
        "graphql-fields": "^2.0.3",
        "graphql-scalars": "^1.9.3",
        "next": "^10.2.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-hook-form": "^7.6.2",
        "reflect-metadata": "^0.1.13",
        "type-graphql": "^1.1.1"
    },
    "scripts": {
        "build": "next build",
        "dev": "concurrently \"next\" \"npm run watch-queries\"",
        "format": "prisma format &&  prettier --write . && eslint --fix .",
        "lint": "eslint .",
        "start": "next start",
        "test": "jest",
        "type-check": "tsc",
        "quality": "npm run type-check && npm run lint && npm run test",
        "generate": "prisma generate",
        "migrate": "prisma migrate dev",
        "deploy": "prisma migrate deploy",
        "reset": "prisma migrate reset",
        "seed": "prisma db seed --preview-feature",
        "studio": "prisma studio",
        "prisma": "prisma format && npm run migrate && npm run seed",
        "generate-types": "graphql-codegen --config codegen.yml",
        "watch-queries": "graphql-codegen --config codegen.yml --watch",
        "postinstall": "prisma generate"
    },
    "devDependencies": {
        "@axe-core/react": "^4.2.0",
        "@babel/core": "^7.14.3",
        "@babel/node": "^7.14.2",
        "@babel/plugin-transform-typescript": "^7.14.3",
        "@babel/preset-env": "^7.14.2",
        "@babel/preset-typescript": "^7.13.0",
        "@graphql-codegen/cli": "^1.21.4",
        "@graphql-codegen/fragment-matcher": "^2.0.1",
        "@graphql-codegen/typescript": "^1.22.0",
        "@graphql-codegen/typescript-operations": "^1.17.16",
        "@graphql-codegen/typescript-react-apollo": "^2.2.4",
        "@testing-library/dom": "^7.30.4",
        "@testing-library/jest-dom": "^5.12.0",
        "@testing-library/react": "^11.2.7",
        "@types/node": "^15.3.1",
        "@types/react": "^17.0.6",
        "@types/react-dom": "^17.0.5",
        "@typescript-eslint/eslint-plugin": "^4.24.0",
        "@typescript-eslint/parser": "^4.24.0",
        "babel-eslint": "^10.1.0",
        "babel-jest": "^26.6.3",
        "concurrently": "^6.0.2",
        "eslint": "^7.25.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^3.4.0",
        "eslint-plugin-react": "^7.23.2",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^26.6.3",
        "prettier": "^2.2.1",
        "prisma": "^2.25.0",
        "ts-node": "^9.1.1",
        "typegraphql-prisma": "^0.14.2",
        "typescript": "^4.2.4"
    }
}
