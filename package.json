{
    "name": "nextjs-typescript-material-ui-eslint-jest",
    "version": "1.0.1",
    "private": true,
    "dependencies": {
        "@material-ui/core": "^4.11.4",
        "@prisma/client": "^2.21.2",
        "next": "~10.1.3",
        "react": "^17.0.2",
        "react-dom": "^17.0.2"
    },
    "scripts": {
        "build": "next build",
        "dev": "next",
        "format": "prisma format &&  prettier --write . && eslint --fix .",
        "lint": "eslint .",
        "start": "next start",
        "test": "jest",
        "type-check": "tsc",
        "quality": "npm run type-check && npm run lint && npm run test",
        "generate": "prisma generate",
        "migrate": "prisma migrate dev --preview-feature",
        "deploy": "prisma migrate deploy --preview-feature",
        "reset": "prisma migrate reset --preview-feature",
        "seed": "prisma db seed --preview-feature",
        "studio": "prisma studio",
        "prisma": "npm run format && npm run generate && npm run migrate && npm run seed"
    },
    "devDependencies": {
        "@babel/core": "^7.13.16",
        "@babel/node": "^7.13.13",
        "@babel/plugin-transform-typescript": "^7.13.0",
        "@babel/preset-env": "^7.13.15",
        "@babel/preset-typescript": "^7.13.0",
        "@testing-library/dom": "^7.30.4",
        "@testing-library/jest-dom": "^5.12.0",
        "@testing-library/react": "^11.2.6",
        "@types/node": "^14.14.43",
        "@types/react": "^17.0.4",
        "@types/react-dom": "^17.0.3",
        "@typescript-eslint/eslint-plugin": "^4.22.0",
        "@typescript-eslint/parser": "^4.22.0",
        "babel-eslint": "^10.1.0",
        "babel-jest": "^26.6.3",
        "eslint": "^7.25.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^3.4.0",
        "eslint-plugin-react": "^7.23.2",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^26.6.3",
        "prettier": "^2.2.1",
        "prisma": "^2.21.2",
        "ts-node": "^9.1.1",
        "typescript": "^4.2.4"
    }
}
